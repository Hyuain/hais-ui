{
  "name": "hais-ui",
  "version": "1.0.0",
  "description": "一个基于 Vue 的简单 UI 框架",
  "main": "dist/index.js",
  "files": [
    "/dist/",
    "/src/",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "parcel index.html --no-cache",
    "build": "rm -rf dist && parcel build index.js --no-cache --no-minify",
    "dev-test": "parcel watch test/* --no-cache & karma start",
    "test": "parcel build test/* --no-minify --no-cache && karma start --single-run",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": "git@github.com:Hyuain/hais-ui.git",
  "author": "Hyuain <Hyuain97@163.com>",
  "license": "MIT",
  "alias": {
    "vue": "./node_modules/vue/dist/vue.js"
  },
  "dependencies": {
    "git-open": "^2.1.0",
    "global": "^4.4.0",
    "sass": "^1.24.0",
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.1.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^6.2.2",
    "node-sass": "^4.13.0",
    "parcel-bundler": "^1.12.4",
    "sass-loader": "^8.0.0",
    "sinon": "^8.0.1",
    "sinon-chai": "^3.4.0",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.3.0"
  }
}
